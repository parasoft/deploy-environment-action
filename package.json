    "@types/node": "^18.7.16",
    "@typescript-eslint/parser": "^4.25.0",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.27.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "jest-circus": "^27.0.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.0",
